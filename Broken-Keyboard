t = int(input())

for _ in range(t):
    s = input()
    i = 0
    res = set()

    while i < len(s):
        j = i
        while j < len(s) and s[j] == s[i]:
            j += 1
        
        if (j - i) % 2 == 1:
            res.add(s[i])
        
        i = j
    ans = list(res)
    print("".join(sorted(ans)))
