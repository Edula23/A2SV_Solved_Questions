class Solution:
    def hIndex(self, citations: List[int]) -> int:
        count = []
        for i in range(1, max(citations)+1):
            c = 0
            for j in range(len(citations)):
                if citations[j] >= i:
                    c+=1
            if c >= i:
                count.append(i)
        if count:
            return max(count)
        else:
            return 0
