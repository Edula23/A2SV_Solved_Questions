t = int(input())
for _ in range(t):
    n , k = map(int, input().split())
    arr = []
    for _ in range(n):
       temp =  list(map(int, input().split()))
       arr.append(temp)
    arr.sort(key=lambda x:x[0])
    for i in range(n):
        if arr[i][0] <= k <= arr[i][1] and arr[i][2] > k:            
            k = arr[i][2]
    print(k)
