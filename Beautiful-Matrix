matrix = []
for i in range(5):
    temp = list(map(int, input().split()))
    matrix.append(temp)
row, col, op = 0, 0, 0
for i in range(5):
    for j in range(5):
        if matrix[i][j] == 1:
            row = i
            col = j
if row == 2:
    op += abs(col-2)
elif col == 2:
    op += abs(row - 2)
else:
    op += abs(col-2)
    op += abs(row-2)
print(op)
